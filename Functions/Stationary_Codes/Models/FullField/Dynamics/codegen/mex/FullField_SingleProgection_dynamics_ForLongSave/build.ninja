# CompilerName=gcc
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/glnxa64
matlabdir = /usr/local/matlab/r2021b
startdir = /home/dp710/NewGitFolder/CavityCodes/Functions/Stationary_Codes/Models/FullField/Dynamics

# Toolchain information
cc = gcc
cxx = /usr/bin/g++
linker = gcc
linkerxx = /usr/bin/g++

cflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -fvisibility=hidden -std=c99 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=FullField_SingleProgection_dynamics_ForLongSave_mex -DMW_NEEDS_VERSION_H -c 

cxxflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -std=c++11 -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -fvisibility=hidden -std=c++11 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=FullField_SingleProgection_dynamics_ForLongSave_mex -DMW_NEEDS_VERSION_H -c 

ldflags =  -pthread -Wl,--no-undefined  -shared -lstdc++ -Wl,--as-needed -Wl,-rpath-link,$matlabdir/bin/glnxa64 -L"$matlabdir/bin/glnxa64" -Wl,-rpath-link,$matlabdir/extern/bin/glnxa64 -L"$matlabdir/extern/bin/glnxa64" -lMatlabDataArray -lmx -lmex -lm -lmat  -o FullField_SingleProgection_dynamics_ForLongSave_mex.mexa64 -lemlrt -lcovrt -lut -lmwmathutil 

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $ldflags

rule linkxx
    command = $linkerxx $in $ldflags

# Build
build $builddir/FullField_SingleProgection_dynamics_ForLongSave_data.o : cxx $root/FullField_SingleProgection_dynamics_ForLongSave_data.cpp
build $builddir/rt_nonfinite.o : cxx $root/rt_nonfinite.cpp
build $builddir/FullField_SingleProgection_dynamics_ForLongSave_initialize.o : cxx $root/FullField_SingleProgection_dynamics_ForLongSave_initialize.cpp
build $builddir/FullField_SingleProgection_dynamics_ForLongSave_terminate.o : cxx $root/FullField_SingleProgection_dynamics_ForLongSave_terminate.cpp
build $builddir/FullField_SingleProgection_dynamics_ForLongSave.o : cxx $root/FullField_SingleProgection_dynamics_ForLongSave.cpp
build $builddir/abs.o : cxx $root/abs.cpp
build $builddir/find.o : cxx $root/find.cpp
build $builddir/fft.o : cxx $root/fft.cpp
build $builddir/mod.o : cxx $root/mod.cpp
build $builddir/_coder_FullField_SingleProgection_dynamics_ForLongSave_api.o : cxx $root/interface/_coder_FullField_SingleProgection_dynamics_ForLongSave_api.cpp
build $builddir/_coder_FullField_SingleProgection_dynamics_ForLongSave_mex.o : cxx $root/interface/_coder_FullField_SingleProgection_dynamics_ForLongSave_mex.cpp
build $builddir/eml_int_forloop_overflow_check.o : cxx $root/eml_int_forloop_overflow_check.cpp
build $builddir/exp.o : cxx $root/exp.cpp
build $builddir/_coder_FullField_SingleProgection_dynamics_ForLongSave_info.o : cxx $root/interface/_coder_FullField_SingleProgection_dynamics_ForLongSave_info.cpp
build $builddir/cpp_mexapi_version.o : cxx $matlabdir/extern/version/cpp_mexapi_version.cpp

# Link
build $root/FullField_SingleProgection_dynamics_ForLongSave_mex.mexa64 : linkxx $builddir/FullField_SingleProgection_dynamics_ForLongSave_data.o $builddir/rt_nonfinite.o $builddir/FullField_SingleProgection_dynamics_ForLongSave_initialize.o $builddir/FullField_SingleProgection_dynamics_ForLongSave_terminate.o $builddir/FullField_SingleProgection_dynamics_ForLongSave.o $builddir/abs.o $builddir/find.o $builddir/fft.o $builddir/mod.o $builddir/_coder_FullField_SingleProgection_dynamics_ForLongSave_api.o $builddir/_coder_FullField_SingleProgection_dynamics_ForLongSave_mex.o $builddir/eml_int_forloop_overflow_check.o $builddir/exp.o $builddir/_coder_FullField_SingleProgection_dynamics_ForLongSave_info.o $builddir/cpp_mexapi_version.o 
