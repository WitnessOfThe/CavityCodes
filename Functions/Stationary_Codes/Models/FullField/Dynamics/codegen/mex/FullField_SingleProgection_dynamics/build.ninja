# CompilerName=gcc
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/glnxa64
matlabdir = /usr/local/matlab/r2021b
startdir = /home/dp710/NewGitFolder/CavityCodes/Functions/Stationary_Codes/Models/FullField/Dynamics

# Toolchain information
cc = /usr/bin/gcc
cxx = g++
linker = /usr/bin/gcc
linkerxx = g++

cflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -std=c99 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=FullField_SingleProgection_dynamics_mex -c 

cxxflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -std=c++11 -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -std=c++11 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=FullField_SingleProgection_dynamics_mex -c 

ldflags =  -pthread -Wl,--no-undefined -Wl,-rpath-link,$matlabdir/bin/glnxa64 -shared  -L"$matlabdir/bin/glnxa64" -lmx -lmex -lmat -lm -lstdc++  -o FullField_SingleProgection_dynamics_mex.mexa64 -lemlrt -lcovrt -lut -lmwmathutil  -Wl,--version-script,FullField_SingleProgection_dynamics_mex.map

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $ldflags

rule linkxx
    command = $linkerxx $in $ldflags

# Build
build $builddir/FullField_SingleProgection_dynamics_data.o : cc $root/FullField_SingleProgection_dynamics_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/FullField_SingleProgection_dynamics_initialize.o : cc $root/FullField_SingleProgection_dynamics_initialize.c
build $builddir/FullField_SingleProgection_dynamics_terminate.o : cc $root/FullField_SingleProgection_dynamics_terminate.c
build $builddir/FullField_SingleProgection_dynamics.o : cc $root/FullField_SingleProgection_dynamics.c
build $builddir/abs.o : cc $root/abs.c
build $builddir/power.o : cc $root/power.c
build $builddir/find.o : cc $root/find.c
build $builddir/fftw.o : cc $root/fftw.c
build $builddir/fft.o : cc $root/fft.c
build $builddir/mod.o : cc $root/mod.c
build $builddir/_coder_FullField_SingleProgection_dynamics_api.o : cc $root/interface/_coder_FullField_SingleProgection_dynamics_api.c
build $builddir/_coder_FullField_SingleProgection_dynamics_mex.o : cc $root/interface/_coder_FullField_SingleProgection_dynamics_mex.c
build $builddir/eml_int_forloop_overflow_check.o : cc $root/eml_int_forloop_overflow_check.c
build $builddir/exp.o : cc $root/exp.c
build $builddir/FullField_SingleProgection_dynamics_emxutil.o : cc $root/FullField_SingleProgection_dynamics_emxutil.c
build $builddir/_coder_FullField_SingleProgection_dynamics_info.o : cc $root/interface/_coder_FullField_SingleProgection_dynamics_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root/FullField_SingleProgection_dynamics_mex.mexa64 : link $builddir/FullField_SingleProgection_dynamics_data.o $builddir/rt_nonfinite.o $builddir/FullField_SingleProgection_dynamics_initialize.o $builddir/FullField_SingleProgection_dynamics_terminate.o $builddir/FullField_SingleProgection_dynamics.o $builddir/abs.o $builddir/power.o $builddir/find.o $builddir/fftw.o $builddir/fft.o $builddir/mod.o $builddir/_coder_FullField_SingleProgection_dynamics_api.o $builddir/_coder_FullField_SingleProgection_dynamics_mex.o $builddir/eml_int_forloop_overflow_check.o $builddir/exp.o $builddir/FullField_SingleProgection_dynamics_emxutil.o $builddir/_coder_FullField_SingleProgection_dynamics_info.o $builddir/c_mexapi_version.o 
