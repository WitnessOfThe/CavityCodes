            t_Save(i).D_mu(i_t)   = d_mu_from_Dyn(tt,ind_t(i_t).ind);
            
        end
        
            d_mu_mean(i) = mean(t_Save(i).D_mu(i_t));  
            d_mu_std(i)  = std(t_Save(i).D_mu(i_t));  
            i
            
    end
    
end
