end
%t_Save =struct(1,61*347);
for i =1:61*347
    
    if  Sim_zone(i) == 1 && isfile(strcat('/home/dp710/Data_Storage/CaF_35_Segment_2E3_kappa/Data/',num2str(i),'.mat')) 
        
        tt=load(strcat('/home/dp710/Data_Storage/CaF_35_Segment_2E3_kappa/Data/',num2str(i),'.mat'));
        
